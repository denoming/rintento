{
  "name": "rintento",
  "version-semver": "1.0.2",
  "license": "MIT",
  "maintainers": [
    "Denys Asauliak <denoming@gmail.com>"
  ],
  "supports": "linux and !windows",
  "dependencies": [
    "jarvisto",
    "boost-core",
    "boost-program-options",
    "boost-json",
    "boost-url",
    "boost-process",
    "boost-circular-buffer",
    "boost-filesystem",
    "boost-uuid",
    {
      "name": "boost-asio",
      "features": [
        "ssl"
      ]
    },
    "spdlog",
    "libconfig"
  ],
  "features": {
    "cli": {
      "description": "CLI supporting",
      "dependencies": [
        {
          "name": "libsndfile",
          "default-features": false
        }
      ]
    },
    "tests": {
      "description": "Tests supporting",
      "dependencies": [
        "gtest",
        {
          "name": "libsndfile",
          "default-features": false
        }
      ]
    }
  }
}
